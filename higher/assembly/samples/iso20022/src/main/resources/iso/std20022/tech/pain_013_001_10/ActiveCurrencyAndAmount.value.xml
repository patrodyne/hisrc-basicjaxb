<cus:customizations
	xmlns:anx="urn:jvnet.org:basicjaxb:xjc:annox"
	xmlns:cus="urn:jvnet.org:basicjaxb:xjc:customizations"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:jvnet.org:basicjaxb:xjc:annox ../../../../../../../doc/annox.xsd
						urn:jvnet.org:basicjaxb:xjc:customizations ../../../../../../../doc/customizations.xsd"
>
	<anx:annotate target="field">
		@jakarta.validation.constraints.Pattern(regexp = "[-+]?[0-9]{1,16}[.][0-9]{2}")
	</anx:annotate>
	<anx:annotatePropertyField>
		@com.fasterxml.jackson.annotation.JsonProperty(required = true)
	</anx:annotatePropertyField>
	<anx:removeAnnotation class="jakarta.validation.constraints.Digits" target="field"/>
</cus:customizations>
