<?xml version="1.0" encoding="UTF-8"?>
<frame
	xmlns="http://www.swixml.org/2007/swixml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.swixml.org/2007/swixml ../../../../../main/resources/swixml.xsd"
	
	plaf="javax.swing.plaf.metal.MetalLookAndFeel"
	plafTheme="org.swixml.plaf.metal.MatteMetalTheme(45,1.25)"
	name="frame"
	size="${el.pageSize(60,20)}"
	preferredSize="${el.size()}"
	title="Simple Complaint Dialog"
	layout="BorderLayout"
	defaultCloseOperation="DISPOSE_ON_CLOSE"
>
	<panel name="pnl" background="*-05-100" border="MatteBorder(4,4,4,4,'*-100-100')" constraints="CENTER" layout="GridBagLayout">
	
		<button name="btn1" text="Wonderful">
			<gridbagconstraints id="gbc_1" insets="2,2,2,2" gridx="0" gridy="0" ipadx="15" ipady="15" weightx="1" weighty="1" />
		</button>
		
		<button name="btn2" text="World">
			<gridbagconstraints refid="gbc_1" gridx="1" />
		</button>
		
		<button name="btn3" text="of">
			<gridbagconstraints insets="2,2,2,2" gridx="0" gridy="1" ipadx="0" ipady="0" weightx="1.0" weighty="1.0" />
		</button>
		
		<button name="btn4" text="Swing">
			<gridbagconstraints insets="2,2,2,2" gridx="1" gridy="1" ipadx="0" ipady="0" weightx="1.0" weighty="1.0" />
		</button>

	</panel>
	
</frame>
