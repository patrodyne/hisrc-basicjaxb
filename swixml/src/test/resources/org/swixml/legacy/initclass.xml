<?xml version="1.0" encoding="UTF-8"?>
<frame
	xmlns="http://www.swixml.org/2007/swixml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.swixml.org/2007/swixml ../../../../../main/resources/swixml.xsd"

	plaf="javax.swing.plaf.metal.MetalLookAndFeel"
	plafTheme="org.swixml.plaf.metal.MatteMetalTheme(105,1.00)"
	size="${el.pageSize(60,12)}"
	preferredSize="${el.size()}"
	layout="BorderLayout"
	title="InitClass Attribute Sample"
	defaultCloseOperation="EXIT_ON_CLOSE"
>

	<panel constraints="CENTER" border="EtchedBorder">
	
		<label font="*-BOLD-125%" foreground="a180-100-100">
			ComboBox
		</label>
		
		<combobox
			initclass="org.swixml.legacy.InitClass$ComboModel"
			action="DO_SELECT"
			prototypeDisplayValue="1234567890|1234567890"
		/>
		
	</panel>
	
	<panel constraints="SOUTH">
		<button>OK</button>
		<button text="Cancel" />
	</panel>

</frame>